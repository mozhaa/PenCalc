<%= content_for :head do %>
  <%= stylesheet_link_tag "navbar" %>
<% end %>

<%= content_for :content do %>
  <div class="nav-layout">
    <%= render partial: "navbar" %>
    
    <div class="page-content">
      <%= content_for?(:page_content) ? yield(:page_content) : yield %>
    </div> 
  </div>
<% end %>

<%= render template: "layouts/application" %>